# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "cuimhne ídithe"

#: 2
msgid "syntax error"
msgstr "earráid chomhréire"

#: 3
msgid "no element found"
msgstr "níor aimsíodh eilimint ar bith"

#: 4
msgid "not well-formed"
msgstr "míchumtha"

#: 5
msgid "unclosed token"
msgstr "comhartha gan dúnadh"

#: 6
msgid "partial character"
msgstr "carachtar neamhiomlán"

#: 7
msgid "mismatched tag"
msgstr "clib mhímheaitseáilte"

#: 8
msgid "duplicate attribute"
msgstr "tréith dhúblach"

#: 9
msgid "junk after document element"
msgstr "ráiméis tar éis na heilimint cáipéise"

#: 10
msgid "illegal parameter entity reference"
msgstr "tagairt mhídhleathach d'aonán paraiméadair"

#: 11
msgid "undefined entity"
msgstr "aonán gan sainmhíniú"

#: 12
msgid "recursive entity reference"
msgstr "tagairt athchúrsach d'aonán"

#: 13
msgid "asynchronous entity"
msgstr "aonán aisionchronach"

#: 14
msgid "reference to invalid character number"
msgstr "tagairt d'uimhir charachtair neamhbhailí"

#: 15
msgid "reference to binary entity"
msgstr "tagairt d'aonán dénártha"

#: 16
msgid "reference to external entity in attribute"
msgstr "tagairt d'aonán seachtrach i dtréith"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "Níl an fógra xml ag tús an aonáin sheachtraigh"

#: 18
msgid "unknown encoding"
msgstr "ionchódú anaithnid"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "Tá an t-ionchódú sonraithe sa bhfógra XML mícheart"

#: 20
msgid "unclosed CDATA section"
msgstr "roinn CDATA gan dúnadh"

#: 21
msgid "error in processing external entity reference"
msgstr "earráid i bpróiseáil de thagairt d'aonán seachtrach"

#: 22
msgid "document is not standalone"
msgstr "ní cáipéis shaorsheasaimh í"

#: 23
msgid "unexpected parser state"
msgstr "staid phársálaí gan súil lei"

#: 24
msgid "entity declared in parameter entity"
msgstr "fograítear aonán in aonán paraiméadair"

#: 27
msgid "prefix not bound to a namespace"
msgstr "níl an réimír ceangailte le hainmspás"

#: 29
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr ""
"ní ceadaítear an réimír choimeádta (xml) a dhífhógairt nó a cheangal le URI "
"ainmspáis eile"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "ní ceadaítear an réimír choimeádta (xmlns) a fhógairt nó a dhífhógairt"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr ""
"ní ceadaítear réimír a cheangal le ceann de na URIanna ainmspáis coimeádta"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#  parsáil, líne-uimhir in Fiontar dict. --kps
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr ""
"Earráid agus XML á pharsáil: %1$S\n"
"Láthair: %2$S\n"
"Líne-Uimhir %3$d, Colún %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Ag súil le: <%S>."

#: No
msgid "Deny"
msgstr "Diúltaigh"

#: Titleline
msgid "Internet Security"
msgstr "Slándáil Idirlín"

#: CheckMessage rememberThisDecision
msgid "Remember this decision"
msgstr "Meabhraigh an cinneadh seo"

#: EnableCapabilityQuery
msgid ""
"A script from \"%S\" is requesting enhanced abilities that are UNSAFE and "
"could be used to compromise your machine or data:\n"
"\n"
"%S\n"
"\n"
"Allow these abilities only if you trust this source to be free of viruses or "
"malicious programs."
msgstr ""
"Tá script as \"%S\" ag lorg cumais fheabhsaithe nach bhfuil sábháilte agus "
"d'fhéadfaí iad a úsáid chun dochar a dhéanamh don ríomhaire nó do chuid "
"sonraí:\n"
"\n"
"%S\n"
"\n"
"Ná ceadaigh na cumais seo ach nuair atá muinín agat go bhfuil an foinse seo "
"saor ó víoras nó ó fheidhmchláir mhailíseacha."

#: EnableCapabilityDenied
msgid "A script from \"%S\" was denied %S privileges."
msgstr "Diúltaíodh pribhléidí do script ó \"%S\"."

#: CheckLoadURIError
msgid "Security Error: Content at %S may not load or link to %S."
msgstr ""
"Earráid slándála: Ní cheadaítear ábhar ag %S a luchtú nó nasc a dhéanamh le %"
"S."

#: CheckSameOriginError
msgid "Security Error: Content at %S may not load data from %S."
msgstr "Earráid slándála: Ní cheadaítear ábhar ag %S sonraí a luchtú ó %S."

#: GetPropertyDenied
msgid "Permission denied to get property %S.%S"
msgstr "Diúltaíodh cead chun cáilíocht %S.%S a fháil"

#: SetPropertyDenied
msgid "Permission denied to set property %S.%S"
msgstr "Diúltaíodh cead chun cáilíocht %S.%S a shocrú"

#: CallMethodDenied
msgid "Permission denied to call method %S.%S"
msgstr "Diúltaíodh cead chun glaoch ar mhodh %S.%S"

#: CreateWrapperDenied
msgid "Permission denied to create wrapper for object of class %S"
msgstr "Diúltaíodh cead chun rapar a chruthú do réad de chineál %S.%S"

#: ExtensionCapability
msgid "Unknown: %S"
msgstr "Anaithnid: %S"

# The following descriptions are shown in the EnableCapabilityQuery dialog
#
#: capdesc.UniversalBrowserRead
msgid "Read private data from any site or window"
msgstr "Léigh sonraí príobháideacha ó shuíomh nó ó fhuinneog ar bith"

#: capdesc.UniversalBrowserWrite
msgid "Modify any open window"
msgstr "Mionathraigh aon fhuinneog atá ar oscailt"

#: capdesc.UniversalXPConnect
msgid "Run or install software on your machine"
msgstr "Déan bogearraí a rith nó a shuiteáil ar do ríomhaire"

#: capdesc.UniversalFileRead
msgid "Read and upload local files"
msgstr "Léigh agus uasluchtaigh comhaid logánta"

#: capdesc.CapabilityPreferencesAccess
msgid "By-pass core security settings"
msgstr "Seachthreoraigh croíshocraithe slándála"

#: capdesc.UniversalPreferencesRead
msgid "Read program settings"
msgstr "Léigh socraithe an fheidhmchláir"

#: capdesc.UniversalPreferencesWrite
msgid "Modify program settings"
msgstr "Mionathraigh socraithe feidhmchláir"

