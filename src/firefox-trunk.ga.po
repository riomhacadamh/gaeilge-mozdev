#: UnsupportedNamespace
msgid ""
"The namespace \"%S\" is not supported. Please use \"http://www.mozilla."
"org/2002/soap/security\"."
msgstr ""
"Ní thacaítear leis an ainmspás \"%S\". Úsáid \"http://www.mozilla.org/2002/"
"soap/security\" le do thoil."

#: UnknownRootElement
msgid ""
"The root element \"%S\" is not recognized. Please use \"webScriptAccess\" "
"element."
msgstr ""
"Ní aithnítear an eilimint fréimhe \"%S\". Úsáid an eilimint \"webScriptAccess"
"\" le do thoil."

#: UnknownElement
msgid "The element \"%S\" is not recognized."
msgstr "Ní aithnítear an eilimint \"%S\"."

#: UnknownAttribute
msgid "The attribute \"%S\" is not recognized."
msgstr "Ní aithnítear an aitreabúid \"%S\"."

#: TooManyElements
msgid "There can be no more than one \"%S\" element."
msgstr "Ní féidir ach eilimint amháin \"%S\" a bheith ann."

#: ElementHasSibling
msgid "\"%S\" element cannot have \"%S\" sibiling."
msgstr "Ní ceadaítear d'eilimint \"%S\" bheith ina deartháireilimint de \"%S\"."

#: ElementNotEmpty
msgid "The \"%S\" element should be empty."
msgstr "Ba cheart don eilimint \"%S\" bheith folamh."

#: SOAPFault
msgid "SOAP { %S } %S call resulted in fault: { %S } %S : %S."
msgstr "Fabht an toradh ón nglao SOAP { %S } %S: { %S } %S : %S."

#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"Stílbhileog %1$S gan luchtú, toisc nach ionann a chineál MIME, \"%2$S\", agus "
"\"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", is "
"not \"text/css\"."
msgstr ""
"Stílbhileog %1$S arna luchtú, cé nach ionann a chineál MIME, \"%2$S\", agus "
"\"text/css\"."

#: PEUnexpEOF
msgid "Unexpected end of file while searching for %1$S."
msgstr "Deireadh comhaid gan súil leis agus %1$S á chuardú."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Tugadh teaghrán gan ach spás bán ann le pársáil mar riail."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Ligeadh an fógra ar lár."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Imithe ar aghaidh go dtí an chéad fógra eile."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Airí anaithnid '%1$S'."

#: PEPropertyParsingError
msgid "Error in parsing value for property '%1$S'."
msgstr "Earráid i bpársáil luacha don airí '%1$S'."

#: PEExpectEndProperty
msgid "Expected end of value for property but found '%1$S'."
msgstr "Bhíothas ag súil le deireadh luacha don airí ach fuarthas '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "deireadh le @-riail anaithnid"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "@-riail anaithnid, nó earráid phársála san @-riail '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "teaghrán tacair charachtar i riail @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Bhíothas ag súil le teaghrán tacair charactar ach fuarthas '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "deireadh le liosta meán i riail @import nó @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Bhíothas ag súil le ',' i liosta na meán ach fuarthas '%1$S'."

